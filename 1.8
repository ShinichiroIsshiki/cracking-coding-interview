import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        // 行列のサイズを入力
        int n = sc.nextInt();
        int m = sc.nextInt();
        
        // 二次元配列を初期化
        int[][] matrix = new int[n][m];
        
        // 配列の各要素を入力
        /*
        期待される入力:
        3
        3
        1 2 3
        4 0 6
        7 8 9
        */
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < m; j++) {
                matrix[i][j] = sc.nextInt();
            }
        }
        
        // 行と列のフラグ配列を初期化
        boolean[] row = new boolean[n];
        boolean[] column = new boolean[m];
        
        // 0を含む行と列を記憶
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < m; j++) {
                if (matrix[i][j] == 0) {
                    row[i] = true;
                    column[j] = true;
                }
            }
        }
        
        // 行を0で埋める
        for (int i = 0; i < n; i++) {
            if (row[i]) {
                for (int j = 0; j < m; j++) {
                    matrix[i][j] = 0;
                }
            }
        }
        
        // 列を0で埋める
        for (int j = 0; j < m; j++) {
            if (column[j]) {
                for (int i = 0; i < n; i++) {
                    matrix[i][j] = 0;
                }
            }
        }
        
        // 結果の出力
        /*
        期待される出力:
        1 0 3
        0 0 0
        7 0 9
        */
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < m; j++) {
                System.out.print(matrix[i][j] + " ");
            }
            System.out.println();
        }
    }
}
